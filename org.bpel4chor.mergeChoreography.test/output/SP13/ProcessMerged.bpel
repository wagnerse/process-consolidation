<?xml version="1.0" encoding="UTF-8"?>
<process
    abstractProcessProfile="urn:HPI_IAAS:choreography:profile:2006/12"
    exitOnStandardFault="yes" name="ProcessMerged"
    suppressJoinFailure="yes"
    targetNamespace="http://www.iaas.uni-stuttgart.de"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/abstract" xmlns:ns="http://www.iaas.uni-stuttgart.de">
    <import importType="http://schemas.xmlsoap.org/wsdl/"
        location="P1SP13.wsdl" namespace="http://www.iaas.uni-stuttgart.de"/>
    <import importType="http://schemas.xmlsoap.org/wsdl/"
        location="P2SP13.wsdl" namespace="http://www.iaas.uni-stuttgart.de"/>
    <variables>
        <variable messageType="ns:P12P2SP13RequestMessage" name="varRec1P2"/>
    </variables>
    <flow name="MergedFlow">
        <links>
            <link name="Invoke1P1SP13TOReceive1P2SP13"/>
        </links>
        <scope exitOnStandardFault="yes" name="Scope_P1SP13" suppressJoinFailure="yes">
            <variables>
                <variable messageType="ns:P1SP13RequestMessage" name="input"/>
                <variable messageType="ns:P1SP13ResponseMessage" name="output"/>
                <variable messageType="ns:P12P2SP13RequestMessage" name="varInv1P1"/>
                <variable messageType="ns:P22P1SP13ResponseMessage" name="varReply1P1"/>
            </variables>
            <partnerLinks>
                <partnerLink initializePartnerRole="no"
                    myRole="P1SP13Provider" name="client"
                    partnerLinkType="ns:P1SP13" partnerRole="P1SP13Requester"/>
            </partnerLinks>
            <faultHandlers>
                <catchAll>
                    <compensate/>
                </catchAll>
            </faultHandlers>
            <flow name="main">
                <links>
                    <link name="link1"/>
                    <link name="link2"/>
                    <link name="link7"/>
                    <link name="link8"/>
                </links>
                <receive createInstance="yes" name="receiveInputP1SP13" variable="input">
                    <sources>
                        <source linkName="link1"/>
                    </sources>
                </receive>
                <empty name="Empty1">
                    <targets>
                        <target linkName="link1"/>
                    </targets>
                    <sources>
                        <source linkName="link2"/>
                    </sources>
                </empty>
                <assign name="Invoke1P1SP13" validate="no">
                    <targets>
                        <target linkName="link2"/>
                        <target linkName="link7"/>
                        <target linkName="link8"/>
                    </targets>
                    <sources>
                        <source linkName="Invoke1P1SP13TOReceive1P2SP13"/>
                    </sources>
                    <copy>
                        <from/>
                        <to variable="varRec1P2"/>
                    </copy>
                </assign>
                <empty name="Empty5">
                    <sources>
                        <source linkName="link7"/>
                    </sources>
                </empty>
                <empty name="Empty6">
                    <sources>
                        <source linkName="link8"/>
                    </sources>
                </empty>
            </flow>
        </scope>
        <scope exitOnStandardFault="yes" name="Scope_P2SP13" suppressJoinFailure="yes">
            <variables>
                <variable messageType="ns:P2SP13RequestMessage" name="input"/>
                <variable messageType="ns:P2SP13ResponseMessage" name="output"/>
                <variable messageType="ns:P22P1SP13ResponseMessage" name="varReply1P2"/>
            </variables>
            <partnerLinks>
                <partnerLink initializePartnerRole="no"
                    myRole="P2SP13Provider" name="client"
                    partnerLinkType="ns:P2SP13" partnerRole="P2SP13Requester"/>
            </partnerLinks>
            <faultHandlers>
                <catchAll>
                    <compensate/>
                </catchAll>
            </faultHandlers>
            <flow name="main">
                <links>
                    <link name="link1"/>
                    <link name="link2"/>
                    <link name="link3"/>
                    <link name="link4"/>
                    <link name="link5"/>
                    <link name="link6"/>
                </links>
                <receive createInstance="yes" name="receiveInputP2SP13" variable="input">
                    <sources>
                        <source linkName="link1"/>
                    </sources>
                </receive>
                <invoke inputVariable="output" name="callbackClientP2SP13">
                    <targets>
                        <target linkName="link6"/>
                    </targets>
                </invoke>
                <empty name="Empty1">
                    <targets>
                        <target linkName="link1"/>
                    </targets>
                    <sources>
                        <source linkName="link2"/>
                    </sources>
                </empty>
                <empty name="Empty2">
                    <targets>
                        <target linkName="link3"/>
                    </targets>
                    <sources>
                        <source linkName="link4"/>
                    </sources>
                </empty>
                <empty name="Receive1P2SP13">
                    <targets>
                        <joinCondition><![CDATA[$Invoke1P1SP13TOReceive1P2SP13 and ($link2)]]></joinCondition>
                        <target linkName="link2"/>
                        <target linkName="Invoke1P1SP13TOReceive1P2SP13"/>
                    </targets>
                    <sources>
                        <source linkName="link3"/>
                    </sources>
                </empty>
                <empty name="Reply1P2SP13">
                    <targets>
                        <target linkName="link4"/>
                    </targets>
                    <sources>
                        <source linkName="link5"/>
                    </sources>
                </empty>
                <empty name="Empty3">
                    <targets>
                        <target linkName="link5"/>
                    </targets>
                    <sources>
                        <source linkName="link6"/>
                    </sources>
                </empty>
            </flow>
        </scope>
    </flow>
</process>
