<?xml version="1.0" encoding="UTF-8"?>
<process
    abstractProcessProfile="urn:HPI_IAAS:choreography:profile:2006/12"
    exitOnStandardFault="yes" name="ProcessMerged"
    suppressJoinFailure="yes"
    targetNamespace="http://www.iaas.uni-stuttgart.de"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/abstract" xmlns:ns="http://www.iaas.uni-stuttgart.de">
    <import importType="http://schemas.xmlsoap.org/wsdl/"
        location="P1ASP15.wsdl" namespace="http://www.iaas.uni-stuttgart.de"/>
    <import importType="http://schemas.xmlsoap.org/wsdl/"
        location="P2ASP15.wsdl" namespace="http://www.iaas.uni-stuttgart.de"/>
    <variables>
        <variable messageType="ns:P2ASP15RequestMessage" name="inputP2ASP15"/>
    </variables>
    <flow name="MergedFlow">
        <links>
            <link name="Invoke1P1ASP15TOEmpty1"/>
        </links>
        <scope exitOnStandardFault="yes" name="Scope_P2ASP15" suppressJoinFailure="yes">
            <variables>
                <variable messageType="ns:P2ASP15ResponseMessage" name="output"/>
                <variable messageType="ns:P12P2ASP15RequestMessage" name="varRec1"/>
            </variables>
            <partnerLinks>
                <partnerLink initializePartnerRole="no"
                    myRole="P2ASP15Provider" name="client"
                    partnerLinkType="ns:P2ASP15" partnerRole="P2ASP15Requester"/>
            </partnerLinks>
            <faultHandlers>
                <catchAll>
                    <compensate/>
                </catchAll>
            </faultHandlers>
            <flow name="main">
                <links>
                    <link name="link2"/>
                </links>
                <empty name="Empty1">
                    <targets>
                        <target linkName="Invoke1P1ASP15TOEmpty1"/>
                    </targets>
                    <sources>
                        <source linkName="link2"/>
                    </sources>
                </empty>
                <empty name="Empty2">
                    <targets>
                        <target linkName="link2"/>
                    </targets>
                </empty>
            </flow>
        </scope>
        <scope exitOnStandardFault="yes" name="Scope_P1ASP15" suppressJoinFailure="yes">
            <variables>
                <variable messageType="ns:P1ASP15RequestMessage" name="inputP1ASP15"/>
                <variable messageType="ns:P1ASP15ResponseMessage" name="output"/>
                <variable messageType="ns:P12P2ASP15RequestMessage" name="varInv1"/>
            </variables>
            <partnerLinks>
                <partnerLink initializePartnerRole="no"
                    myRole="P1ASP15Provider" name="client"
                    partnerLinkType="ns:P1ASP15" partnerRole="P1ASP15Requester"/>
            </partnerLinks>
            <faultHandlers>
                <catchAll>
                    <compensate/>
                </catchAll>
            </faultHandlers>
            <flow name="main">
                <links>
                    <link name="link1"/>
                    <link name="link2"/>
                    <link name="link3"/>
                </links>
                <receive createInstance="yes" name="receiveInputP1ASP15" variable="inputP1ASP15">
                    <sources>
                        <source linkName="link1"/>
                    </sources>
                </receive>
                <empty name="Empty1">
                    <targets>
                        <target linkName="link1"/>
                    </targets>
                    <sources>
                        <source linkName="link2"/>
                    </sources>
                </empty>
                <assign name="Invoke1P1ASP15" validate="no">
                    <targets>
                        <target linkName="link2"/>
                    </targets>
                    <sources>
                        <source linkName="link3"/>
                        <source linkName="Invoke1P1ASP15TOEmpty1"/>
                    </sources>
                    <copy>
                        <from variable="varInv1"/>
                        <to variable="inputP2ASP15"/>
                    </copy>
                </assign>
                <empty name="Empty2">
                    <targets>
                        <target linkName="link3"/>
                    </targets>
                </empty>
            </flow>
        </scope>
    </flow>
</process>
