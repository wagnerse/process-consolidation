<?xml version="1.0" encoding="UTF-8"?>
<process
    abstractProcessProfile="urn:HPI_IAAS:choreography:profile:2006/12"
    exitOnStandardFault="yes" name="ProcessMerged"
    suppressJoinFailure="yes"
    targetNamespace="http://www.iaas.uni-stuttgart.de"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/abstract"
    xmlns:ns="http://www.iaas.uni-stuttgart.de" xmlns:ns0="http://www.w3.org/2001/XMLSchema">
    <import importType="http://schemas.xmlsoap.org/wsdl/"
        location="P2ASP18.wsdl" namespace="http://www.iaas.uni-stuttgart.de"/>
    <import importType="http://schemas.xmlsoap.org/wsdl/"
        location="P1ASP18.wsdl" namespace="http://www.iaas.uni-stuttgart.de"/>
    <import importType="http://schemas.xmlsoap.org/wsdl/"
        location="P3ASP18.wsdl" namespace="http://www.iaas.uni-stuttgart.de"/>
    <variables>
        <variable messageType="ns:CommonASP18RequestMessage" name="varRec1P2"/>
        <variable name="varRec1P2_written" type="ns0:boolean">
            <from><![CDATA[false()]]></from>
        </variable>
    </variables>
    <flow name="MergedFlow">
        <links>
            <link name="Invoke1P1ASP18TOReceive1P2ASP18"/>
            <link name="Invoke1P3ASP18TOReceive1P2ASP18"/>
        </links>
        <scope exitOnStandardFault="yes" name="Scope_P2ASP18" suppressJoinFailure="yes">
            <variables>
                <variable messageType="ns:P2ASP18RequestMessage" name="input"/>
                <variable messageType="ns:P2ASP18ResponseMessage" name="output"/>
            </variables>
            <partnerLinks>
                <partnerLink initializePartnerRole="no"
                    myRole="P2ASP18Provider" name="client"
                    partnerLinkType="ns:P2ASP18" partnerRole="P2ASP18Requester"/>
            </partnerLinks>
            <faultHandlers>
                <catchAll>
                    <compensate/>
                </catchAll>
            </faultHandlers>
            <flow name="main">
                <links>
                    <link name="link1"/>
                    <link name="link2"/>
                    <link name="link3"/>
                    <link name="link4"/>
                    <link name="link5"/>
                </links>
                <receive createInstance="yes" name="receiveInputP2ASP18" variable="input">
                    <sources>
                        <source linkName="link1"/>
                    </sources>
                </receive>
                <invoke inputVariable="output" name="callbackClientP2ASP18">
                    <targets>
                        <target linkName="link4"/>
                    </targets>
                </invoke>
                <empty name="Empty1">
                    <targets>
                        <target linkName="link1"/>
                    </targets>
                    <sources>
                        <source linkName="link2"/>
                    </sources>
                </empty>
                <empty name="Empty2">
                    <targets>
                        <target linkName="link3"/>
                    </targets>
                    <sources>
                        <source linkName="link4"/>
                    </sources>
                </empty>
                <scope name="Receive1P2ASP18">
                    <targets>
                        <target linkName="link2"/>
                        <target linkName="link5"/>
                    </targets>
                    <sources>
                        <source linkName="link3"/>
                    </sources>
                    <faultHandlers>
                        <catchAll>
                            <empty/>
                        </catchAll>
                    </faultHandlers>
                    <flow>
                        <throw faultName="joinFailure">
                            <targets>
                                <target linkName="Invoke1P1ASP18TOReceive1P2ASP18"/>
                            </targets>
                        </throw>
                        <throw faultName="joinFailure">
                            <targets>
                                <target linkName="Invoke1P3ASP18TOReceive1P2ASP18"/>
                            </targets>
                        </throw>
                    </flow>
                </scope>
                <empty name="Empty3">
                    <sources>
                        <source linkName="link5"/>
                    </sources>
                </empty>
            </flow>
        </scope>
        <scope exitOnStandardFault="yes" name="Scope_P1ASP18" suppressJoinFailure="yes">
            <variables>
                <variable messageType="ns:P1ASP18RequestMessage" name="input"/>
                <variable messageType="ns:P1ASP18ResponseMessage" name="output"/>
                <variable messageType="ns:CommonASP18RequestMessage" name="varInv1"/>
            </variables>
            <partnerLinks>
                <partnerLink initializePartnerRole="no"
                    myRole="P1ASP18Provider" name="client"
                    partnerLinkType="ns:P1ASP18" partnerRole="P1ASP18Requester"/>
            </partnerLinks>
            <faultHandlers>
                <catchAll>
                    <compensate/>
                </catchAll>
            </faultHandlers>
            <flow name="main">
                <links>
                    <link name="link1"/>
                    <link name="link2"/>
                    <link name="link3"/>
                    <link name="link4"/>
                </links>
                <receive createInstance="yes" name="receiveInputP1ASP18" variable="input">
                    <sources>
                        <source linkName="link1"/>
                    </sources>
                </receive>
                <invoke inputVariable="output" name="callbackClientP1ASP18">
                    <targets>
                        <target linkName="link4"/>
                    </targets>
                </invoke>
                <empty name="Empty1">
                    <targets>
                        <target linkName="link1"/>
                    </targets>
                    <sources>
                        <source linkName="link2"/>
                    </sources>
                </empty>
                <if name="Invoke1P1ASP18">
                    <targets>
                        <target linkName="link2"/>
                    </targets>
                    <sources>
                        <source linkName="link3"/>
                    </sources>
                    <condition><![CDATA[varRec1P2_written!=true()]]></condition>
                    <assign validate="no">
                        <sources>
                            <source linkName="Invoke1P1ASP18TOReceive1P2ASP18"/>
                        </sources>
                        <copy>
                            <from><![CDATA[true()]]></from>
                            <to variable="varRec1P2_written"/>
                        </copy>
                        <copy>
                            <from variable="varInv1"/>
                            <to variable="varRec1P2"/>
                        </copy>
                    </assign>
                </if>
                <empty name="Empty2">
                    <targets>
                        <target linkName="link3"/>
                    </targets>
                    <sources>
                        <source linkName="link4"/>
                    </sources>
                </empty>
            </flow>
        </scope>
        <scope exitOnStandardFault="yes" name="Scope_P3ASP18" suppressJoinFailure="yes">
            <variables>
                <variable messageType="ns:P3ASP18RequestMessage" name="input"/>
                <variable messageType="ns:P3ASP18ResponseMessage" name="output"/>
                <variable messageType="ns:CommonASP18RequestMessage" name="varInv1"/>
            </variables>
            <partnerLinks>
                <partnerLink initializePartnerRole="no"
                    myRole="P3ASP18Provider" name="client"
                    partnerLinkType="ns:P3ASP18" partnerRole="P3ASP18Requester"/>
            </partnerLinks>
            <faultHandlers>
                <catchAll>
                    <compensate/>
                </catchAll>
            </faultHandlers>
            <flow name="main">
                <links>
                    <link name="link1"/>
                    <link name="link2"/>
                    <link name="link3"/>
                    <link name="link4"/>
                </links>
                <receive createInstance="yes" name="receiveInputP3ASP18" variable="input">
                    <sources>
                        <source linkName="link1"/>
                    </sources>
                </receive>
                <invoke inputVariable="output" name="callbackClientP3ASP18">
                    <targets>
                        <target linkName="link4"/>
                    </targets>
                </invoke>
                <empty name="Empty1">
                    <targets>
                        <target linkName="link1"/>
                    </targets>
                    <sources>
                        <source linkName="link2"/>
                    </sources>
                </empty>
                <if name="Invoke1P3ASP18">
                    <targets>
                        <target linkName="link2"/>
                    </targets>
                    <sources>
                        <source linkName="link3"/>
                    </sources>
                    <condition><![CDATA[varRec1P2_written!=true()]]></condition>
                    <assign validate="no">
                        <sources>
                            <source linkName="Invoke1P3ASP18TOReceive1P2ASP18"/>
                        </sources>
                        <copy>
                            <from><![CDATA[true()]]></from>
                            <to variable="varRec1P2_written"/>
                        </copy>
                        <copy>
                            <from variable="varInv1"/>
                            <to variable="varRec1P2"/>
                        </copy>
                    </assign>
                </if>
                <empty name="Empty2">
                    <targets>
                        <target linkName="link3"/>
                    </targets>
                    <sources>
                        <source linkName="link4"/>
                    </sources>
                </empty>
            </flow>
        </scope>
    </flow>
</process>
