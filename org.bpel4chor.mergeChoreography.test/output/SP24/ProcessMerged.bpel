<?xml version="1.0" encoding="UTF-8"?>
<process
    abstractProcessProfile="urn:HPI_IAAS:choreography:profile:2006/12"
    exitOnStandardFault="yes" name="ProcessMerged"
    suppressJoinFailure="yes"
    targetNamespace="http://www.iaas.uni-stuttgart.de"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/abstract"
    xmlns:ns="http://www.iaas.uni-stuttgart.de" xmlns:ns0="http://www.w3.org/2001/XMLSchema">
    <import importType="http://schemas.xmlsoap.org/wsdl/"
        location="P2SP24.wsdl" namespace="http://www.iaas.uni-stuttgart.de"/>
    <import importType="http://schemas.xmlsoap.org/wsdl/"
        location="P1SP24.wsdl" namespace="http://www.iaas.uni-stuttgart.de"/>
    <variables>
        <variable messageType="ns:P12P2SP24RequestMessage" name="varRec1P2"/>
        <variable messageType="ns:P22P1SP24ResponseMessage" name="varReply1P1"/>
        <variable name="PickvVST_activated" type="ns0:boolean">
            <from><![CDATA[false()]]></from>
        </variable>
    </variables>
    <flow name="MergedFlow">
        <links>
            <link name="Invoke1P1SP24TOPick"/>
            <link name="Reply1P2SP24TOThrow"/>
            <link name="Reply2P2SP24TOThrow"/>
        </links>
        <scope exitOnStandardFault="yes" name="Scope_P2SP24" suppressJoinFailure="yes">
            <variables>
                <variable messageType="ns:P2SP24RequestMessage" name="input"/>
                <variable messageType="ns:P2SP24ResponseMessage" name="output"/>
                <variable name="varReply1P2"/>
            </variables>
            <partnerLinks>
                <partnerLink initializePartnerRole="no"
                    myRole="P2SP24Provider" name="client"
                    partnerLinkType="ns:P2SP24" partnerRole="P2SP24Requester"/>
            </partnerLinks>
            <faultHandlers>
                <catchAll>
                    <compensate/>
                </catchAll>
            </faultHandlers>
            <flow name="main">
                <links>
                    <link name="link1"/>
                    <link name="link4"/>
                    <link name="link2"/>
                    <link name="link5"/>
                    <link name="link3"/>
                    <link name="link6"/>
                    <link name="link7"/>
                </links>
                <receive createInstance="yes" name="receiveInputP2SP24" variable="input">
                    <sources>
                        <source linkName="link1"/>
                    </sources>
                </receive>
                <invoke inputVariable="output" name="callbackClientP2SP24">
                    <targets>
                        <target linkName="link4"/>
                    </targets>
                </invoke>
                <empty name="Empty1">
                    <targets>
                        <target linkName="link1"/>
                    </targets>
                    <sources>
                        <source linkName="link2"/>
                    </sources>
                </empty>
                <empty name="Empty2">
                    <targets>
                        <target linkName="link6"/>
                    </targets>
                    <sources>
                        <source linkName="link4"/>
                    </sources>
                </empty>
                <empty name="Empty3">
                    <sources>
                        <source linkName="link5"/>
                    </sources>
                </empty>
                <scope name="Pick">
                    <targets>
                        <target linkName="link2"/>
                        <target linkName="link5"/>
                    </targets>
                    <sources>
                        <source linkName="link3"/>
                        <source linkName="link7"/>
                    </sources>
                    <faultHandlers>
                        <catch faultName="ns:Pick1P2SP24Fault">
                            <empty name="EmptyPick1"/>
                        </catch>
                        <catch faultName="ns:Pick2P2SP24Fault">
                            <empty name="EmptyPick2"/>
                        </catch>
                        <catch faultName="ns:Pick3P2SP24Fault">
                            <empty name="EmptyPick3"/>
                        </catch>
                        <catch faultName="ns:wait0Fault">
                            <scope exitOnStandardFault="no" isolated="no">
                                <empty name="EmptyPick4"/>
                            </scope>
                        </catch>
                        <catch faultName="ns:wait1Fault">
                            <scope exitOnStandardFault="no" isolated="no">
                                <empty name="EmptyPick5"/>
                            </scope>
                        </catch>
                    </faultHandlers>
                    <flow>
                        <sequence>
                            <receive createInstance="no"
                                name="Pick2P2SP24" variable="varRec1P2"/>
                            <assign validate="no">
                                <copy>
                                    <from><![CDATA[true()]]></from>
                                    <to variable="PickvVST_activated"/>
                                </copy>
                            </assign>
                            <throw faultName="ns:Pick2P2SP24Fault"/>
                        </sequence>
                        <sequence>
                            <receive createInstance="no"
                                name="Pick3P2SP24" variable="varRec1P2"/>
                            <assign validate="no">
                                <copy>
                                    <from><![CDATA[true()]]></from>
                                    <to variable="PickvVST_activated"/>
                                </copy>
                            </assign>
                            <throw faultName="ns:Pick3P2SP24Fault"/>
                        </sequence>
                        <sequence>
                            <wait>
                                <for><![CDATA['P3DT10H']]></for>
                            </wait>
                            <assign validate="no">
                                <copy>
                                    <from><![CDATA[true()]]></from>
                                    <to variable="PickvVST_activated"/>
                                </copy>
                            </assign>
                            <throw faultName="ns:wait0Fault"/>
                        </sequence>
                        <sequence>
                            <wait>
                                <until><![CDATA['2008-10-28T10:28:19']]></until>
                            </wait>
                            <assign validate="no">
                                <copy>
                                    <from><![CDATA[true()]]></from>
                                    <to variable="PickvVST_activated"/>
                                </copy>
                            </assign>
                            <throw faultName="ns:wait1Fault"/>
                        </sequence>
                        <throw faultName="ns:Pick1P2SP24Fault">
                            <targets>
                                <target linkName="Invoke1P1SP24TOPick"/>
                            </targets>
                        </throw>
                    </flow>
                </scope>
                <assign name="Reply1P2SP24" validate="no">
                    <targets>
                        <target linkName="link3"/>
                    </targets>
                    <sources>
                        <source linkName="link6"/>
                        <source linkName="Reply1P2SP24TOThrow"/>
                    </sources>
                    <copy>
                        <from variable="varReply1P2"/>
                        <to variable="varReply1P1"/>
                    </copy>
                </assign>
                <assign name="Reply2P2SP24" validate="no">
                    <targets>
                        <target linkName="link7"/>
                    </targets>
                    <sources>
                        <source linkName="Reply2P2SP24TOThrow"/>
                    </sources>
                    <copy>
                        <from variable="varReply1P2"/>
                        <to variable="varReply1P1"/>
                    </copy>
                </assign>
            </flow>
        </scope>
        <scope exitOnStandardFault="yes" name="Scope_P1SP24" suppressJoinFailure="yes">
            <variables>
                <variable messageType="ns:P1SP24RequestMessage" name="input"/>
                <variable messageType="ns:P1SP24ResponseMessage" name="output"/>
                <variable messageType="ns:P12P2SP24RequestMessage" name="varInv1P1"/>
            </variables>
            <partnerLinks>
                <partnerLink initializePartnerRole="no"
                    myRole="P1SP24Provider" name="client"
                    partnerLinkType="ns:P1SP24" partnerRole="P1SP24Requester"/>
            </partnerLinks>
            <faultHandlers>
                <catchAll>
                    <compensate/>
                </catchAll>
            </faultHandlers>
            <flow name="main">
                <links>
                    <link name="link1"/>
                    <link name="link2"/>
                    <link name="link4"/>
                    <link name="link3"/>
                </links>
                <receive createInstance="yes" name="receiveInputP1SP24" variable="input">
                    <sources>
                        <source linkName="link1"/>
                    </sources>
                </receive>
                <invoke inputVariable="output" name="callbackClientP1SP24">
                    <targets>
                        <target linkName="link4"/>
                    </targets>
                </invoke>
                <empty name="Empty1">
                    <targets>
                        <target linkName="link1"/>
                    </targets>
                    <sources>
                        <source linkName="link2"/>
                    </sources>
                </empty>
                <empty name="Empty2">
                    <targets>
                        <target linkName="link3"/>
                    </targets>
                    <sources>
                        <source linkName="link4"/>
                    </sources>
                </empty>
                <scope name="Invoke1P1SP24">
                    <targets>
                        <target linkName="link2"/>
                    </targets>
                    <sources>
                        <source linkName="link3"/>
                    </sources>
                    <faultHandlers>
                        <catch faultName="ns:exitFault">
                            <empty/>
                        </catch>
                    </faultHandlers>
                    <sequence>
                        <if name="Invoke1P1SP24">
                            <condition><![CDATA[PickvVST_activated!=true()]]></condition>
                            <assign name="Invoke1P1SP24" validate="no">
                                <sources>
                                    <source linkName="Invoke1P1SP24TOPick"/>
                                </sources>
                                <copy>
                                    <from variable="varInv1P1"/>
                                    <to variable="varRec1P2"/>
                                </copy>
                            </assign>
                        </if>
                        <flow>
                            <throw faultName="ns:exitFault">
                                <targets>
                                    <target linkName="Reply1P2SP24TOThrow"/>
                                </targets>
                            </throw>
                            <throw faultName="ns:exitFault">
                                <targets>
                                    <target linkName="Reply2P2SP24TOThrow"/>
                                </targets>
                            </throw>
                        </flow>
                    </sequence>
                </scope>
            </flow>
        </scope>
    </flow>
</process>
