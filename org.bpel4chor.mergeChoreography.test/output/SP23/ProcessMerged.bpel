<?xml version="1.0" encoding="UTF-8"?>
<process
    abstractProcessProfile="urn:HPI_IAAS:choreography:profile:2006/12"
    exitOnStandardFault="yes" name="ProcessMerged"
    suppressJoinFailure="yes"
    targetNamespace="http://www.iaas.uni-stuttgart.de"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/abstract"
    xmlns:ns="http://www.iaas.uni-stuttgart.de" xmlns:ns0="http://www.w3.org/2001/XMLSchema">
    <import importType="http://schemas.xmlsoap.org/wsdl/"
        location="P2SP23.wsdl" namespace="http://www.iaas.uni-stuttgart.de"/>
    <import importType="http://schemas.xmlsoap.org/wsdl/"
        location="P1SP23.wsdl" namespace="http://www.iaas.uni-stuttgart.de"/>
    <variables>
        <variable messageType="ns:P12P2SP23RequestMessage" name="varRec1P2"/>
        <variable name="PickzMG2_activated" type="ns0:boolean">
            <from><![CDATA[false()]]></from>
        </variable>
    </variables>
    <flow name="MergedFlow">
        <links>
            <link name="Invoke1P1SP23TOPick"/>
        </links>
        <scope exitOnStandardFault="yes" name="Scope_P2SP23" suppressJoinFailure="yes">
            <variables>
                <variable messageType="ns:P2SP23RequestMessage" name="input"/>
                <variable messageType="ns:P2SP23ResponseMessage" name="output"/>
                <variable name="varReply1P2"/>
            </variables>
            <partnerLinks>
                <partnerLink initializePartnerRole="no"
                    myRole="P2SP23Provider" name="client"
                    partnerLinkType="ns:P2SP23" partnerRole="P2SP23Requester"/>
            </partnerLinks>
            <faultHandlers>
                <catchAll>
                    <compensate/>
                </catchAll>
            </faultHandlers>
            <flow name="main">
                <links>
                    <link name="link1"/>
                    <link name="link4"/>
                    <link name="link2"/>
                    <link name="link5"/>
                    <link name="link3"/>
                    <link name="link6"/>
                </links>
                <receive createInstance="yes" name="receiveInputP2SP23" variable="input">
                    <sources>
                        <source linkName="link1"/>
                    </sources>
                </receive>
                <invoke inputVariable="output" name="callbackClientP2SP23">
                    <targets>
                        <target linkName="link4"/>
                    </targets>
                </invoke>
                <empty name="Empty1">
                    <targets>
                        <target linkName="link1"/>
                    </targets>
                    <sources>
                        <source linkName="link2"/>
                    </sources>
                </empty>
                <empty name="Empty2">
                    <targets>
                        <target linkName="link6"/>
                    </targets>
                    <sources>
                        <source linkName="link4"/>
                    </sources>
                </empty>
                <empty name="Empty3">
                    <sources>
                        <source linkName="link5"/>
                    </sources>
                </empty>
                <scope name="Pick">
                    <targets>
                        <target linkName="link2"/>
                        <target linkName="link5"/>
                    </targets>
                    <sources>
                        <source linkName="link3"/>
                    </sources>
                    <faultHandlers>
                        <catch faultName="ns:Pick1P2SP23Fault">
                            <empty name="EmptyPick1"/>
                        </catch>
                        <catch faultName="ns:Pick2P2SP23Fault">
                            <empty name="EmptyPick2"/>
                        </catch>
                        <catch faultName="ns:Pick3P2SP23Fault">
                            <empty name="EmptyPick3"/>
                        </catch>
                        <catch faultName="ns:wait0Fault">
                            <scope exitOnStandardFault="no" isolated="no">
                                <empty name="EmptyPick4"/>
                            </scope>
                        </catch>
                        <catch faultName="ns:wait1Fault">
                            <scope exitOnStandardFault="no" isolated="no">
                                <empty name="EmptyPick5"/>
                            </scope>
                        </catch>
                    </faultHandlers>
                    <flow>
                        <sequence>
                            <receive createInstance="no"
                                name="Pick2P2SP23" variable="varRec1P2"/>
                            <assign validate="no">
                                <copy>
                                    <from><![CDATA[true()]]></from>
                                    <to variable="PickzMG2_activated"/>
                                </copy>
                            </assign>
                            <throw faultName="ns:Pick2P2SP23Fault"/>
                        </sequence>
                        <sequence>
                            <receive createInstance="no"
                                name="Pick3P2SP23" variable="varRec1P2"/>
                            <assign validate="no">
                                <copy>
                                    <from><![CDATA[true()]]></from>
                                    <to variable="PickzMG2_activated"/>
                                </copy>
                            </assign>
                            <throw faultName="ns:Pick3P2SP23Fault"/>
                        </sequence>
                        <sequence>
                            <wait>
                                <for><![CDATA['P3DT10H']]></for>
                            </wait>
                            <assign validate="no">
                                <copy>
                                    <from><![CDATA[true()]]></from>
                                    <to variable="PickzMG2_activated"/>
                                </copy>
                            </assign>
                            <throw faultName="ns:wait0Fault"/>
                        </sequence>
                        <sequence>
                            <wait>
                                <until><![CDATA['2008-10-28T10:28:19']]></until>
                            </wait>
                            <assign validate="no">
                                <copy>
                                    <from><![CDATA[true()]]></from>
                                    <to variable="PickzMG2_activated"/>
                                </copy>
                            </assign>
                            <throw faultName="ns:wait1Fault"/>
                        </sequence>
                        <throw faultName="ns:Pick1P2SP23Fault">
                            <targets>
                                <target linkName="Invoke1P1SP23TOPick"/>
                            </targets>
                        </throw>
                    </flow>
                </scope>
                <empty name="Reply1P2SP23">
                    <targets>
                        <target linkName="link3"/>
                    </targets>
                    <sources>
                        <source linkName="link6"/>
                    </sources>
                </empty>
            </flow>
        </scope>
        <scope exitOnStandardFault="yes" name="Scope_P1SP23" suppressJoinFailure="yes">
            <variables>
                <variable messageType="ns:P1SP23RequestMessage" name="input"/>
                <variable messageType="ns:P1SP23ResponseMessage" name="output"/>
                <variable messageType="ns:P12P2SP23RequestMessage" name="varInv1P1"/>
                <variable messageType="ns:P22P1SP23ResponseMessage" name="varReply1P1"/>
            </variables>
            <partnerLinks>
                <partnerLink initializePartnerRole="no"
                    myRole="P1SP23Provider" name="client"
                    partnerLinkType="ns:P1SP23" partnerRole="P1SP23Requester"/>
            </partnerLinks>
            <faultHandlers>
                <catchAll>
                    <compensate/>
                </catchAll>
            </faultHandlers>
            <flow name="main">
                <links>
                    <link name="link1"/>
                    <link name="link2"/>
                </links>
                <receive createInstance="yes" name="receiveInputP1SP23" variable="input">
                    <sources>
                        <source linkName="link1"/>
                    </sources>
                </receive>
                <empty name="Empty1">
                    <targets>
                        <target linkName="link1"/>
                    </targets>
                    <sources>
                        <source linkName="link2"/>
                    </sources>
                </empty>
                <if name="Invoke1P1SP23">
                    <targets>
                        <target linkName="link2"/>
                    </targets>
                    <condition><![CDATA[PickzMG2_activated!=true()]]></condition>
                    <assign validate="no">
                        <sources>
                            <source linkName="Invoke1P1SP23TOPick"/>
                        </sources>
                        <copy>
                            <from/>
                            <to variable="varRec1P2"/>
                        </copy>
                    </assign>
                </if>
            </flow>
        </scope>
    </flow>
</process>
