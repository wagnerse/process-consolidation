<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://uni-stuttgart.de/bpel/InitVar" xmlns:vprop="http://docs.oasis-open.org/wsbpel/2.0/varprop" xmlns:wsdl="http://webservice.iaas.uni_stuttgart.de" name="InitVar" targetNamespace="http://uni-stuttgart.de/bpel/InitVar">
        
    <plnk:partnerLinkType name="BpePLT">
    <plnk:role name="bpeProvider" portType="wsdl:BpelPrinterException"/>
  </plnk:partnerLinkType>
    <import location="BpelPrinterException.wsdl" namespace="http://webservice.iaas.uni_stuttgart.de"/>
    <types>
        <schema xmlns="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://uni-stuttgart.de/bpel/InitVar">

            <element name="InitVarRequest">
                <complexType>
                    <sequence>
                        <element name="input" type="string"/>
                    </sequence>
                </complexType>
            </element>
        </schema>
    </types>
 
    <message name="InitVarRequestMessage">
        <part element="tns:InitVarRequest" name="payload"/>
    </message>

    <portType name="InitVar">
        <operation name="process">
            <input message="tns:InitVarRequestMessage"/>
        </operation>
    </portType>   
    
    <plnk:partnerLinkType name="InitVar">
        <plnk:role name="InitVarProvider" portType="tns:InitVar"/>
    </plnk:partnerLinkType>
    
    <binding name="InitVarSOAPBinding" type="tns:InitVar">
    	<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    	<operation name="process">
    		<soap:operation soapAction="http://uni-stuttgart.de/bpel/InitVar/process"/>
    		<input>
    			<soap:body use="literal"/>
    		</input>
    	</operation>
    </binding>

    <service name="InitVarProcessService">
    	<port binding="tns:InitVarSOAPBinding" name="InitVarProcessPort">
    		<soap:address location="http://localhost:8080/ode/processes/InitVar"/>
    	</port>
    </service>
</definitions>
